    "tableSchema": {
      "columns": [
      {
        "name": "incArriveDestination",
        "datatype": "string"
      },
      {
        "name": "incDCALeftScene",
        "datatype": "string"
      },
      {
        "name": "incDCAOnScene",
        "datatype": "string"
      },
      {
        "name": "incLocationType",
        "datatype": "string"
      },
      {
        "name": "incRRVOnScene",
        "datatype": "string"
      },
      {
        "name": "incTimePatSide",
        "datatype": "string"
      }
      ]
    }
  }],
  "dialect": {
    "header": true
  }
}
