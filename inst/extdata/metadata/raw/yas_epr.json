        "columns": [
          {
            "datatype": "string",
            "name": "incArriveDestination"
          },
          {
            "datatype": "string",
            "name": "incDCALeftScene"
          },
          {
            "datatype": "string",
            "name": "incDCAOnScene"
          },
          {
            "datatype": "string",
            "name": "incLocationType"
          },
          {
            "datatype": "string",
            "name": "incRRVOnScene"
          },
          {
            "datatype": "time",
            "name": "incTimePatSide"
          }
        ]
      },
      "url": "yas_epr/Incident.csv"
    }
  ]
}
